{
    "workbench.activityBar.location": "hidden",
    "breadcrumbs.enabled": false,
    "editor.minimap.enabled": false,
    "editor.scrollbar.vertical": "hidden",
    "editor.scrollbar.horizontal": "hidden",
    "workbench.editor.showTabs": "single",
    "window.menuBarVisibility": "toggle",
    "editor.lineNumbers": "relative",
    "vim.useSystemClipboard": true,
    "vim.hlsearch": true,
    "vim.leader": "<space>",
    "vim.normalModeKeyBindingsNonRecursive": [
        { "before": ["<leader>", "e"], "commands": ["workbench.view.explorer"] },
        { "before": ["<leader>", "f"], "commands": ["workbench.action.quickOpen"] },
        { "before": ["<leader>", "w"], "commands": ["workbench.action.files.save"] },
        { "before": ["<C-h>"], "commands": ["workbench.action.navigateLeft"] },
        { "before": ["<C-l>"], "commands": ["workbench.action.navigateRight"] },
        { "before": ["<C-k>"], "commands": ["workbench.action.navigateUp"] },
        { "before": ["<C-j>"], "commands": ["workbench.action.navigateDown"] }
    ],
    "editor.lineHeight": 1.6,
    "editor.cursorBlinking": "solid",
    "workbench.startupEditor": "none",
    "workbench.colorCustomizations": {
        "focusBorder": "{{colors.primary.default.hex}}",
        "foreground": "{{colors.on_surface.default.hex}}",
        "editor.background": "{{colors.surface.default.hex}}",
        "editor.foreground": "{{colors.on_surface.default.hex}}",
        "editor.lineHighlightBackground": "{{colors.surface_container_high.default.hex}}",
        "editor.selectionBackground": "{{colors.primary_container.default.hex}}40",
        "editorBracketMatch.background": "{{colors.primary_container.default.hex}}40",
        "editorBracketMatch.border": "{{colors.primary.default.hex}}",
        "editorCursor.foreground": "{{colors.primary.default.hex}}",
        "editorLineNumber.foreground": "{{colors.outline.default.hex}}",
        "editorLineNumber.activeForeground": "{{colors.primary.default.hex}}",
        "activityBar.background": "{{colors.surface_container_low.default.hex}}",
        "activityBar.foreground": "{{colors.primary.default.hex}}",
        "activityBar.inactiveForeground": "{{colors.on_surface_variant.default.hex}}",
        "activityBarBadge.background": "{{colors.primary.default.hex}}",
        "activityBarBadge.foreground": "{{colors.on_primary.default.hex}}",
        "sideBar.background": "{{colors.surface_container_low.default.hex}}",
        "sideBar.foreground": "{{colors.on_surface.default.hex}}",
        "sideBarTitle.foreground": "{{colors.primary.default.hex}}",
        "sideBarSectionHeader.background": "{{colors.surface_container.default.hex}}",
        "sideBarSectionHeader.foreground": "{{colors.on_surface.default.hex}}",
        "list.activeSelectionBackground": "{{colors.primary.default.hex}}",
        "list.activeSelectionForeground": "{{colors.on_primary.default.hex}}",
        "list.inactiveSelectionBackground": "{{colors.surface_container_high.default.hex}}",
        "list.hoverBackground": "{{colors.surface_container_highest.default.hex}}",
        "statusBar.background": "{{colors.surface_container_low.default.hex}}",
        "statusBar.foreground": "{{colors.on_surface.default.hex}}",
        "statusBar.noFolderBackground": "{{colors.surface_container_low.default.hex}}",
        "titleBar.activeBackground": "{{colors.surface_container_low.default.hex}}",
        "titleBar.activeForeground": "{{colors.on_surface.default.hex}}",
        "titleBar.inactiveBackground": "{{colors.surface_container_low.default.hex}}",
        "titleBar.inactiveForeground": "{{colors.on_surface_variant.default.hex}}",
        "tab.activeBackground": "{{colors.surface.default.hex}}",
        "tab.activeForeground": "{{colors.primary.default.hex}}",
        "tab.activeBorderTop": "{{colors.primary.default.hex}}",
        "tab.inactiveBackground": "{{colors.surface_container_low.default.hex}}",
        "tab.inactiveForeground": "{{colors.on_surface_variant.default.hex}}",
        "tab.border": "{{colors.surface_container_low.default.hex}}",
        "editorGroupHeader.tabsBackground": "{{colors.surface_container_low.default.hex}}",
        "editorWidget.background": "{{colors.surface_container.default.hex}}",
        "editorWidget.border": "{{colors.outline.default.hex}}",
        "editorSuggestWidget.background": "{{colors.surface_container.default.hex}}",
        "editorSuggestWidget.selectedBackground": "{{colors.surface_container_high.default.hex}}",
        "editorSuggestWidget.highlightForeground": "{{colors.primary.default.hex}}",
        "editorHoverWidget.background": "{{colors.surface_container.default.hex}}",
        "editorHoverWidget.border": "{{colors.outline.default.hex}}",
        "input.background": "{{colors.surface_container_high.default.hex}}",
        "input.foreground": "{{colors.on_surface.default.hex}}",
        "input.border": "{{colors.outline.default.hex}}",
        "inputOption.activeBorder": "{{colors.primary.default.hex}}",
        "button.background": "{{colors.primary.default.hex}}",
        "button.foreground": "{{colors.on_primary.default.hex}}",
        "button.hoverBackground": "{{colors.primary_container.default.hex}}",
        "dropdown.background": "{{colors.surface_container.default.hex}}",
        "dropdown.border": "{{colors.primary.default.hex}}",
        "panel.background": "{{colors.surface.default.hex}}",
        "panel.border": "{{colors.outline.default.hex}}",
        "panelTitle.activeForeground": "{{colors.primary.default.hex}}",
        "panelTitle.activeBorder": "{{colors.primary.default.hex}}",
        "terminal.foreground": "{{colors.on_surface.default.hex}}",
        "terminal.background": "{{colors.surface.default.hex}}",
        "terminal.ansiBlack": "#45475a",
        "terminal.ansiBrightBlack": "#585b70",
        "terminal.ansiRed": "#f38ba8",
        "terminal.ansiBrightRed": "#f37799",
        "terminal.ansiGreen": "#a6e3a1",
        "terminal.ansiBrightGreen": "#89d88b",
        "terminal.ansiYellow": "#f9e2af",
        "terminal.ansiBrightYellow": "#ebd391",
        "terminal.ansiBlue": "#89b4fa",
        "terminal.ansiBrightBlue": "#74a8fc",
        "terminal.ansiMagenta": "#f5c2e7",
        "terminal.ansiBrightMagenta": "#f2aede",
        "terminal.ansiCyan": "#94e2d5",
        "terminal.ansiBrightCyan": "#6bd7ca",
        "terminal.ansiWhite": "#a6adc8",
        "terminal.ansiBrightWhite": "#bac2de",
        "terminal.selectionBackground": "{{colors.primary_container.default.hex}}80",
        "terminalCursor.foreground": "{{colors.primary.default.hex}}",
        "scrollbarSlider.background": "{{colors.outline.default.hex}}80",
        "scrollbarSlider.hoverBackground": "{{colors.outline.default.hex}}",
        "scrollbarSlider.activeBackground": "{{colors.primary.default.hex}}80"
    },
    "editor.tokenColorCustomizations": {
        "comments": "#9399b2",
        "keywords": "#cba6f7",
        "strings": "#a6e3a1",
        "numbers": "#fab387",
        "functions": "#89b4fa",
        "types": "#f9e2af",
        "variables": "#cdd6f4",
        "textMateRules": [
            {
                "scope": ["text", "source", "variable.other.readwrite", "punctuation.definition.variable"],
                "settings": {
                    "foreground": "#cdd6f4"
                }
            },
            {
                "scope": "punctuation",
                "settings": {
                    "foreground": "#9399b2"
                }
            },
            {
                "scope": ["comment", "punctuation.definition.comment"],
                "settings": {
                    "foreground": "#9399b2",
                    "fontStyle": "italic"
                }
            },
            {
                "scope": ["string", "punctuation.definition.string"],
                "settings": {
                    "foreground": "#a6e3a1"
                }
            },
            {
                "scope": "constant.character.escape",
                "settings": {
                    "foreground": "#f5c2e7"
                }
            },
            {
                "scope": ["constant.numeric", "variable.other.constant", "entity.name.constant", "constant.language.boolean"],
                "settings": {
                    "foreground": "#fab387"
                }
            },
            {
                "scope": ["keyword", "keyword.operator.word", "keyword.operator.new", "variable.language.super", "support.type.primitive", "storage.type", "storage.modifier", "punctuation.definition.keyword"],
                "settings": {
                    "foreground": "#cba6f7"
                }
            },
            {
                "scope": ["keyword.operator", "punctuation.accessor", "punctuation.definition.generic", "punctuation.separator.key-value"],
                "settings": {
                    "foreground": "#94e2d5"
                }
            },
            {
                "scope": ["entity.name.function", "meta.function-call.method", "support.function", "variable.function"],
                "settings": {
                    "foreground": "#89b4fa",
                    "fontStyle": "italic"
                }
            },
            {
                "scope": ["entity.name.class", "entity.other.inherited-class", "support.class", "meta.function-call.constructor", "entity.name.struct", "entity.name.enum"],
                "settings": {
                    "foreground": "#f9e2af",
                    "fontStyle": "italic"
                }
            },
            {
                "scope": ["variable.other.enummember", "meta.enum variable.other.readwrite"],
                "settings": {
                    "foreground": "#94e2d5"
                }
            },
            {
                "scope": ["meta.property.object"],
                "settings": {
                    "foreground": "#94e2d5"
                }
            },
            {
                "scope": ["meta.type", "meta.type-alias", "support.type", "entity.name.type"],
                "settings": {
                    "foreground": "#f9e2af",
                    "fontStyle": "italic"
                }
            },
            {
                "scope": ["meta.annotation variable.function", "meta.decorator", "punctuation.decorator"],
                "settings": {
                    "foreground": "#fab387"
                }
            },
            {
                "scope": ["variable.parameter", "meta.function.parameters"],
                "settings": {
                    "foreground": "#eba0ac",
                    "fontStyle": "italic"
                }
            },
            {
                "scope": ["constant.language", "support.function.builtin"],
                "settings": {
                    "foreground": "#f38ba8"
                }
            },
            {
                "scope": ["entity.name.namespace"],
                "settings": {
                    "foreground": "#f9e2af"
                }
            },
            {
                "scope": ["variable.language.this"],
                "settings": {
                    "foreground": "#f38ba8"
                }
            },
            {
                "scope": ["keyword.other.definition.ini", "support.type.property-name.json", "support.type.property-name.toml", "entity.name.tag.yaml"],
                "settings": {
                    "foreground": "#89b4fa"
                }
            },
            {
                "scope": ["constant.language.json", "constant.language.yaml"],
                "settings": {
                    "foreground": "#fab387"
                }
            },
            {
                "scope": ["support.type.property-name.table", "entity.name.section.group-title.ini"],
                "settings": {
                    "foreground": "#f9e2af"
                }
            },
            {
                "scope": ["support.type.property-name.css"],
                "settings": {
                    "foreground": "#89b4fa"
                }
            },
            {
                "scope": ["source.css entity.other.attribute-name.class.css"],
                "settings": {
                    "foreground": "#f9e2af"
                }
            },
            {
                "scope": ["entity.name.tag"],
                "settings": {
                    "foreground": "#89b4fa"
                }
            },
            {
                "scope": ["entity.other.attribute-name"],
                "settings": {
                    "foreground": "#f9e2af"
                }
            },
            {
                "scope": ["markup.heading"],
                "settings": {
                    "foreground": "#f38ba8",
                    "fontStyle": "bold"
                }
            },
            {
                "scope": ["markup.bold"],
                "settings": {
                    "foreground": "#f38ba8",
                    "fontStyle": "bold"
                }
            },
            {
                "scope": ["markup.italic"],
                "settings": {
                    "foreground": "#f38ba8",
                    "fontStyle": "italic"
                }
            },
            {
                "scope": ["markup.underline.link"],
                "settings": {
                    "foreground": "#89b4fa"
                }
            },
            {
                "scope": ["markup.inline.raw.string.markdown"],
                "settings": {
                    "foreground": "#a6e3a1"
                }
            },
            {
                "scope": ["variable.language.special.self.python"],
                "settings": {
                    "foreground": "#f38ba8",
                    "fontStyle": "italic"
                }
            },
            {
                "scope": ["support.function.magic.python"],
                "settings": {
                    "foreground": "#89dceb",
                    "fontStyle": "italic"
                }
            },
            {
                "scope": ["entity.name.function.decorator.python", "punctuation.definition.decorator.python"],
                "settings": {
                    "foreground": "#fab387",
                    "fontStyle": "italic"
                }
            }
        ]
    }
}

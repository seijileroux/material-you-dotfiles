        "panelTitle.activeForeground": "{{colors.primary.default.hex}}",
        "terminal.foreground": "{{colors.on_surface.default.hex}}",
        "terminal.background": "{{colors.surface.default.hex}}",
        "terminal.ansiBlack": "#4c4c4c",
        "terminal.ansiBrightBlack": "#262626",
        "terminal.ansiRed": "#E88594",
        "terminal.ansiBrightRed": "#E88594",
        "terminal.ansiGreen": "#A3D393",
        "terminal.ansiBrightGreen": "#A3D393",
        "terminal.ansiYellow": "#E9CE9D",
        "terminal.ansiBrightYellow": "#E9CE9D",
        "terminal.ansiBlue": "{{colors.primary.default.hex}}",
        "terminal.ansiBrightBlue": "{{colors.primary_container.default.hex}}",
        "terminal.ansiMagenta": "#C3A0F3",
        "terminal.ansiBrightMagenta": "#C3A0F3",
        "terminal.ansiCyan": "#81C2E1",
        "terminal.ansiBrightCyan": "#81C2E1",
        "terminal.ansiWhite": "#e7e7e7",
        "terminal.ansiBrightWhite": "#f0f0f0",

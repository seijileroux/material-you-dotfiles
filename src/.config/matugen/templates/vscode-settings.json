        "terminalCursor.background": "{{colors.on_surface_variant.default.hex}}",
        "scrollbarSlider.background": "{{colors.surface_container_highest.default.hex}}80",
        "scrollbarSlider.hoverBackground": "{{colors.surface_container_highest.default.hex}}",
        "scrollbarSlider.activeBackground": "{{colors.primary.default.hex}}80"
    },
    "editor.tokenColorCustomizations": {
        "comments": "#4c4c4c",
        "keywords": "{{colors.primary.default.hex}}",
        "strings": "#A3D393",
        "numbers": "#E9CE9D",
        "functions": "{{colors.secondary.default.hex}}",
        "types": "#81C2E1",
        "variables": "{{colors.on_surface.default.hex}}",
        "textMateRules": [
            {
                "scope": ["comment", "punctuation.definition.comment"],
                "settings": {
                    "foreground": "#4c4c4c",
                    "fontStyle": "italic"
                }
            },
            {
                "scope": ["keyword", "storage.type", "storage.modifier", "keyword.control", "keyword.operator.new"],
                "settings": {
                    "foreground": "{{colors.primary.default.hex}}"
                }
            },
            {
                "scope": ["entity.name.function", "support.function", "meta.function-call"],
                "settings": {
                    "foreground": "{{colors.secondary.default.hex}}"
                }
            },
            {
                "scope": ["entity.name.type", "entity.name.class", "support.class", "support.type"],
                "settings": {
                    "foreground": "#81C2E1"
                }
            },
            {
                "scope": ["string", "string.quoted", "string.template"],
                "settings": {
                    "foreground": "#A3D393"
                }
            },
            {
                "scope": ["constant.numeric", "constant.language.numeric"],
                "settings": {
                    "foreground": "#E9CE9D"
                }
            },
            {
                "scope": ["constant.language", "constant.language.boolean", "constant.language.null"],
                "settings": {
                    "foreground": "#E88594"
                }
            },
            {
                "scope": ["variable", "variable.other.readwrite", "variable.other.object"],
                "settings": {
                    "foreground": "{{colors.on_surface.default.hex}}"
                }
            },
            {
                "scope": ["variable.parameter", "variable.other.constant"],
                "settings": {
                    "foreground": "#C3A0F3"
                }
            },
            {
                "scope": ["entity.name.tag", "meta.tag", "punctuation.definition.tag"],
                "settings": {
                    "foreground": "{{colors.primary.default.hex}}"
                }
            },
            {
                "scope": ["entity.other.attribute-name"],
                "settings": {
                    "foreground": "{{colors.secondary.default.hex}}",
                    "fontStyle": "italic"
                }
            },
            {
                "scope": ["punctuation.definition.string", "punctuation.definition.template-expression"],
                "settings": {
                    "foreground": "#A3D393"
                }
            },
            {
                "scope": ["support.constant", "constant.other"],
                "settings": {
                    "foreground": "#C3A0F3"
                }
            },
            {
                "scope": ["markup.heading"],
                "settings": {
                    "foreground": "{{colors.primary.default.hex}}",
                    "fontStyle": "bold"
                }
            },
            {
                "scope": ["markup.bold"],
                "settings": {
                    "foreground": "{{colors.secondary.default.hex}}",
                    "fontStyle": "bold"
                }
            },
            {
                "scope": ["markup.italic"],
                "settings": {
                    "foreground": "#A3D393",
                    "fontStyle": "italic"
                }
            },
            {
                "scope": ["meta.import", "meta.require"],
                "settings": {
                    "foreground": "#C3A0F3"
                }
            },
            {
                "scope": ["variable.language.this", "variable.language.super"],
                "settings": {
                    "foreground": "#E88594",
                    "fontStyle": "italic"
                }
            }
        ]
    }
}
